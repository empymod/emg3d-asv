{
    // Version of the config file format.
    "version": 1,

    // Name of the project being benchmarked.
    "project": "emg3d",

    // Project's homepage.
    "project_url": "http://empymod.github.io",

    // URL of the source code repository for the project being benchmarked.
    "repo": "https://github.com/empymod/emg3d.git",

    // Tool to use to create environments.
    "environment_type": "conda",

    // Base URL to show a commit for the project.
    "show_commit_url": "http://github.com/empymod/emg3d/commit/",

    // Python version(s).
    "pythons": ["3.7",],

    // Directory to store raw benchmark results.
    "results_dir": "../emg3d-bench/results",
}
